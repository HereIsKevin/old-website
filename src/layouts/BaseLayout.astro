---
import Head from "../components/Head.astro"
import Nav from "../components/Nav.astro"
import "../styles/base.css"

export interface Props {
  title: string
}

const { title } = Astro.props
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <Head {title} />
  </head>
  <body>
    <Nav />
    <slot />
  </body>
</html>
