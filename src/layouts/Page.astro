---
import Head from "../components/Head.astro"
import Header from "../components/Header.astro"
import "../styles/global.css"

export interface Props {
  title: string
}

const { title } = Astro.props
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <Head {title} />
  </head>
  <body>
    <Header />
    <main>
      <slot />
    </main>
  </body>
</html>
