{
  "name": "hereiskevin.github.io",
  "version": "0.1.0",
  "private": true,
  "description": "Kevin's personal blog and website.",
  "repository": "HereIsKevin/hereiskevin.github.io",
  "license": "UNLICENSED",
  "author": "Kevin Feng",
  "type": "module",
  "scripts": {
    "build": "astro build",
    "check": "npm run check:astro && npm run check:tsc",
    "check:astro": "astro check",
    "check:tsc": "tsc --noEmit",
    "clean": "rimraf ./.astro/ ./dist/",
    "format": "prettier --write --ignore-path ./.gitignore \"./**/*.{astro,js,json,md,svelte,ts}\"",
    "lint": "eslint --fix --ignore-path ./.gitignore \"./**/*.{astro,svelte,ts}\"",
    "preview": "astro preview",
    "start": "astro dev",
    "watch:astro": "astro check --watch",
    "watch:tsc": "tsc --noEmit --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@astrojs/rss": "^2.4.3",
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/svelte": "^3.1.0",
    "@astrojs/tailwind": "^4.0.0",
    "@fontsource/jetbrains-mono": "^5.0.3",
    "@fontsource/lato": "^5.0.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^2.7.2",
    "sharp": "^0.32.1",
    "svelte": "^4.0.1",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-svelte": "^2.32.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
